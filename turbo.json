{
  "$schema": "https://turborepo.com/schema.json",
  "globalDependencies": [
    "**/.env.*"
  ],
  "tasks": {
    "build": {

      "passThroughEnv": [
        "CLERK_SECRET_KEY",
        "SUPABASE_SERVICE_KEY",
        "ENCRYPTION_KEY",
        "SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_URL",
        "NEXT_PUBLIC_SUPABASE_ANON_KEY",
        "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY"
      ],
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "check-types": {}
  }
}